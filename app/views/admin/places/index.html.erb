<h1>Place</h1>

<table class="table">
  <thead class="thead-light">
    <tr>
      <th>Name</th>
      <th>Address</th>
      <th>Tel</th>
      <th>Information</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @places.each do |place| %>
      <tr>
        <td><%= place.name %></td>
        <td><%= place.address %></td>
        <td><%= place.tel%></td>
        <td><%= place.info %></td>
        <td><%= place.latitude %></td>
        <td><%= place.longitude %></td>
        <td><%= link_to 'Edit', edit_place_path(place) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New place', new_place_path %>